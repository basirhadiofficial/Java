class MyMainEmployee{
   private int id;
   private String name;
   public MyMainEmployee(){    // creating a constructor
       id = 45;
       name = "your name here:";
   }

   public String getName() {
       return name;
   }
   public void setName(String name) {
       this.name = name;
   }
   public int getId() {
       return id;
   }
   public void setId(int id) {
       this.id = id;
   }
}

public class OOPs {
   public static void main(String[] args) {
       MyMainEmployee basir = new MyMainEmployee();
//        basir.setName("basir hadi");
//        basir.setId(24);
       System.out.println(basir.getId());
       System.out.println(basir.getName());
   }
}
